{
  "git.ignoreLimitWarning": true,
  "[javascript]": {
    "editor.tabSize": 2,
  },
  "editor.formatOnSave": false,
  "eslint.enable": true,
  "eslint.workingDirectories": [
    "./testapp/"
  ],
  "files.associations": {
    "*.vue": "vue"
  },
  "eslint.options": {
    "configFile": ".eslintrc.js"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  //"vetur.validation.template": false
}